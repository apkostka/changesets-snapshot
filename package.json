{
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "commitizen": "^4.2.4",
    "skuba": "4.3.0",
    "tsup": "^6.1.3"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@seek/changesets-snapshot",
  "scripts": {
    "build": "tsup",
    "commit": "cz",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "yarn build && skuba release",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "sideEffects": false,
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": null,
    "type": "package",
    "version": "4.3.0"
  },
  "version": "0.0.0-semantically-released",
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  }
}
